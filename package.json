  },
  "license": "BSD-2-Clause",
  "keywords": [
    "excalibur",
    "game",
    "engine",
    "typescript",
    "javascript",
    "simple"
  ],
  "scripts": {
    "all": "grunt",
    "core": "grunt core",
    "lint": "grunt lint",
    "travis": "grunt travis",
    "visual": "grunt visual",
    "sandbox": "grunt sandbox",
    "start": "grunt watch",
    "test": "karma start",
    "apidocs": "grunt apidocs",
